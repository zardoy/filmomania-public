{
    "name": "filmomania",
    "author": "Vitaly Turovsky",
    "version": "0.0.1-beta",
    "repository": "https://github.com/zardoy/filmomania-public.git",
    "description": "best hub for movies",
    "license": "MIT",
    "private": true,
    "build": {
        "productName": "FilmoMania Beta",
        "appId": "com.zardoy.filmomania",
        "win": {
            "target": "nsis"
        },
        "files": [
            "build/**/*",
            "dist/**/*"
        ],
        "directories": {
            "buildResources": "assets",
            "output": "build-app"
        }
    },
    "dependencies": {
        "@emotion/css": "^11.1.3",
        "@emotion/react": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "@fontsource/roboto": "^4.5.0",
        "@material-ui/core": "^5.0.0-beta.1",
        "@material-ui/icons": "^5.0.0-beta.1",
        "@vitejs/plugin-react-refresh": "^1.3.5",
        "@zardoy/proxy-util": "https://github.com/zardoy/proxy-utils#build",
        "@zardoy/simple-formik": "https://github.com/zardoy/simple-formik#build",
        "@zardoy/tsconfig": "^1.1.1",
        "axios": "^0.21.1",
        "cheerio": "^1.0.0-rc.10",
        "clsx": "^1.1.1",
        "download": "^8.0.0",
        "electron-debug": "^3.2.0",
        "electron-is-dev": "^2.0.0",
        "electron-store": "^8.0.0",
        "electron-unhandled": "^3.0.2",
        "electron-util": "^0.17.0",
        "electron-window-keeper": "https://github.com/zardoy/electron-window-keeper.git#build",
        "esbuild": "^0.12.15",
        "execa": "^5.0.0",
        "filesize": "^7.0.0",
        "formik": "^2.2.6",
        "framer-motion": "^4.1.17",
        "git-remote-origin-url": "^3.1.0",
        "got": "^11.8.1",
        "is-online": "^9.0.1",
        "json-schema-typed": "^7.0.3",
        "lodash": "^4.17.20",
        "material-ui-popup-state": "^1.9.0",
        "modify-json-file": "^1.1.0",
        "normalize-url": "^7.0.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-hook-form": "^7.11.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-use": "^17.2.4",
        "rimraf": "^3.0.2",
        "sanitize-filename": "^1.6.3",
        "typed-ipc": "^0.0.3",
        "underscore": "^1.12.0",
        "use-typed-event-listener": "^3.0.0",
        "xbytes": "^1.7.0",
        "zustand": "^3.5.7"
    },
    "devDependencies": {
        "@codeshift/utils": "^0.1.2",
        "@types/ffmpeg-static": "^3.0.0",
        "@types/jscodeshift": "^0.11.2",
        "@types/lodash": "^4.14.171",
        "@types/node": "^16.4.0",
        "@types/react": "^17.0.14",
        "@types/react-dom": "^17.0.9",
        "@types/react-router-dom": "^5.1.8",
        "@types/rimraf": "^3.0.1",
        "@types/tunnel": "^0.0.3",
        "@types/underscore": "^1.11.3",
        "@typescript-eslint/eslint-plugin": "^4.28.4",
        "@typescript-eslint/parser": "^4.28.4",
        "autoprefixer": "^10.3.1",
        "electron": "^13.1.7",
        "electron-builder": "^22.9.1",
        "electron-builder-squirrel-windows": "^22.10.5",
        "eslint": "^7.31.0",
        "eslint-plugin-react": "^7.22.0",
        "postcss": "^8.3.6",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "tailwindcss": "^2.2.6",
        "ts-node": "^10.1.0",
        "tsc-watch": "^4.2.9",
        "type-fest": "^1.2.2",
        "typescript": "^4.4.0-dev.20210725",
        "vite": "^2.4.3"
    },
    "scripts": {
        "start-main": "tsc-watch -p ./src/electron/tsconfig.json --onSuccess \"node electron-esbuild.mjs\"",
        "start-renderer": "vite",
        "build-main": "tsc -p ./src/electron/tsconfig.build.json",
        "build-renderer": "craco-electron build",
        "analyze-renderer": "source-map-explorer 'build/static/js/*.js'",
        "run-local-test": "cd local-tests && ts-node --transpile-only index.ts",
        "build-app": "yarn build-main && yarn build-renderer && yarn electron-pack",
        "electron-pack": "electron-builder --win"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "development": [
            "last 1 chrome version"
        ],
        "production": [
            "last 1 chrome version"
        ]
    }
}
