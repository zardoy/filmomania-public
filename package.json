{
    "name": "filmomania",
    "main": "dist/electron/index.js",
    "author": "Vitaly Turovsky",
    "version": "0.0.1-beta",
    "repository": "https://github.com/zardoy/filmomania-public.git",
    "description": "best hub for movies",
    "license": "MIT",
    "private": true,
    "build": {
        "productName": "FilmoMania Beta",
        "appId": "com.zardoy.filmomania",
        "win": {
            "target": "nsis"
        },
        "files": [
            "build/**/*",
            "dist/**/*"
        ],
        "directories": {
            "buildResources": "assets",
            "output": "build-app"
        }
    },
    "dependencies": {
        "axios": "^0.21.1",
        "cheerio": "^1.0.0-rc.5",
        "craco-electron": "^1.0.2",
        "download": "^8.0.0",
        "electron-debug": "^3.2.0",
        "electron-is-dev": "^1.2.0",
        "electron-store": "^7.0.2",
        "electron-unhandled": "^3.0.2",
        "electron-util": "^0.14.2",
        "electron-window-state": "^5.0.3",
        "execa": "^5.0.0",
        "filesize": "^6.1.0",
        "got": "^11.8.1",
        "lodash": "^4.17.20",
        "rimraf": "^3.0.2",
        "sanitize-filename": "^1.6.3",
        "soda-player-patch": "https://github.com/zardoy/soda-player-plus#build",
        "typed-ipc": "https://github.com/zardoy/typed-ipc#build",
        "underscore": "^1.12.0",
        "xbytes": "^1.7.0",
        "@apollo/client": "^3.3.9",
        "graphql": "^15.5.0",
        "@fontsource/roboto": "^4.2.1",
        "@material-ui/core": "^4.11.3",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.57",
        "formik": "^2.2.6",
        "framer-motion": "^3.3.0",
        "material-ui-popup-state": "^1.7.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0"
    },
    "devDependencies": {
        "@types/electron-window-state": "^2.0.34",
        "@types/ffmpeg-static": "^3.0.0",
        "@types/lodash": "^4.14.168",
        "@types/node": "^14.14.27",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.1",
        "@types/react-router": "^5.1.11",
        "@types/react-router-dom": "^5.1.7",
        "@types/rimraf": "^3.0.0",
        "@types/tunnel": "^0.0.1",
        "@types/underscore": "^1.10.24",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "@typescript-eslint/parser": "^4.15.0",
        "electron": "^11.2.3",
        "electron-builder": "^22.9.1",
        "electron-builder-squirrel-windows": "^22.10.5",
        "eslint": "^7.19.0",
        "eslint-plugin-react": "^7.22.0",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9",
        "type-fest": "^0.21.0",
        "typescript": "^4.1.5"
    },
    "scripts": {
        "start-main": "tsc-watch -p ./src/electron/tsconfig.json --onSuccess \"electron .\"",
        "start-renderer": "craco-electron start",
        "build-main": "tsc -p ./src/electron/tsconfig.build.json",
        "build-renderer": "craco-electron build",
        "analyze-renderer": "source-map-explorer 'build/static/js/*.js'",
        "run-local-test": "cd local-tests && ts-node --transpile-only index.ts",
        "build-app": "yarn build-main && yarn build-renderer && yarn electron-pack",
        "electron-pack": "electron-builder --win"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "development": [
            "last 1 chrome version"
        ],
        "production": [
            "last 1 chrome version"
        ]
    }
}
