{
    "name": "filmomania",
    "author": "Vitaly Turovsky",
    "version": "0.0.1-beta",
    "repository": "https://github.com/zardoy/filmomania-public.git",
    "description": "best hub for movies",
    "license": "MIT",
    "private": true,
    "main": "build/index.js",
    "build": {
        "productName": "FilmoMania Beta",
        "appId": "com.zardoy.filmomania",
        "artifactName": "${productName}-${version}-${platform}-${arch}.${ext}",
        "asarUnpack": [
            "build/settingsSchema.json",
            "dist/overlay.html",
            "dist-remote-ui/"
        ],
        "win": {
            "target": "nsis"
        },
        "mac": {
            "target": {
                "target": "dmg",
                "arch": [
                    "arm64"
                ]
            }
        },
        "nsis": {},
        "files": [
            "build/**/*",
            "dist/**/*",
            "dist-remote-ui/**/*"
        ],
        "directories": {
            "buildResources": "assets",
            "output": "release"
        },
        "publish": {
            "provider": "github",
            "releaseType": "release"
        }
    },
    "dependencies": {
        "@emotion/css": "^11.1.3",
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@fontsource/roboto": "^4.5.0",
        "@mui/icons-material": "^5.0.0-rc.0",
        "@mui/material": "^5.0.0-rc.0",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "@zardoy/electron-esbuild": "^0.1.0",
        "@zardoy/proxy-util": "github:zardoy/proxy-utils#build",
        "@zardoy/react-util": "^0.2.0",
        "@zardoy/simple-formik": "github:zardoy/simple-formik#build",
        "@zardoy/tsconfig": "^1.2.1",
        "@zardoy/utils": "^0.0.11",
        "@zardoy/vit": "^0.1.8",
        "axios": "^0.21.1",
        "cheerio": "^1.0.0-rc.10",
        "clsx": "^1.1.1",
        "download": "^8.0.0",
        "electron-debug": "^3.2.0",
        "electron-is-dev": "^2.0.0",
        "electron-store": "^8.0.0",
        "electron-unhandled": "^3.0.2",
        "electron-util": "^0.17.2",
        "electron-window-keeper": "github:zardoy/electron-window-keeper#build",
        "exit-hook": "^3.1.4",
        "fastclick": "^1.0.6",
        "filesize": "^8.0.0",
        "framer-motion": "^4.1.17",
        "git-remote-origin-url": "^3.1.0",
        "got": "^11.8.2",
        "i18next": "^22.4.5",
        "i18next-http-backend": "^2.1.0",
        "is-online": "^9.0.1",
        "json-schema-typed": "^7.0.3",
        "kill-port": "^2.0.1",
        "lodash": "^4.17.21",
        "material-ui-popup-state": "^5.0.4",
        "modify-json-file": "^1.1.0",
        "mpv": "^1.0.0",
        "normalize-url": "^7.0.1",
        "parse-torrent": "^10.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-i18next": "^12.1.1",
        "react-router": "^5.2.1",
        "react-router-dom": "^5.2.1",
        "react-use": "^17.3.1",
        "rimraf": "^3.0.2",
        "sanitize-filename": "^1.6.3",
        "serve-handler": "^6.1.5",
        "slash": "^5.0.0",
        "systeminformation": "^5.16.9",
        "tabbable": "^6.0.1",
        "twin.macro": "^3.1.0",
        "typed-ipc": "^0.0.3",
        "underscore": "^1.13.1",
        "use-typed-event-listener": "^3.0.0",
        "valtio": "^1.7.6",
        "vite-plugin-externals": "^0.5.1",
        "ws": "^8.11.0",
        "xbytes": "^1.7.0",
        "zustand": "^3.5.10"
    },
    "devDependencies": {
        "esbuild": "^0.16.9",
        "@codeshift/utils": "^0.1.2",
        "@types/ffmpeg-static": "^3.0.0",
        "@types/lodash": "^4.14.172",
        "@types/node": "^16.7.10",
        "@types/parse-torrent": "^5.8.4",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@types/react-router-dom": "^5.1.8",
        "@types/serve-handler": "^6.1.1",
        "@types/tunnel": "^0.0.3",
        "@types/underscore": "^1.11.3",
        "@types/ws": "^8.5.4",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.30.0",
        "autoprefixer": "^10.3.4",
        "electron": "^22.0.0",
        "electron-builder": "23",
        "electron-builder-squirrel-windows": "23",
        "eslint": "^7.32.0",
        "eslint-plugin-react": "^7.25.1",
        "postcss": "^8.4.20",
        "tailwindcss": "^3.2.4",
        "ts-node": "^10.9.1",
        "tsc-watch": "^4.5.0",
        "tsx": "^3.12.1",
        "type-fest": "^2.1.0",
        "typescript": "^4.9.4",
        "vite": "^4.0.2"
    },
    "scripts": {
        "start-main": "electron-esbuild --debug",
        "start-renderer": "vite",
        "start-remote-control-ui": "vite --config vite.config.remote-ui.ts --host",
        "build-main": "tsc -p ./src/electron && node build-electron.mjs && tsx buildSettingsSchema.ts",
        "build-renderer": "tsc -p ./src/react && vite build",
        "build-remote-control-ui": "tsc -p ./src/remote-control/ && vite --config vite.config.remote-ui.ts build",
        "analyze-renderer": "source-map-explorer 'build/static/js/*.js'",
        "lint": "eslint src/**",
        "build-app": "pnpm build-main && pnpm build-renderer && pnpm build-remote-control-ui && pnpm electron-pack",
        "electron-pack": "electron-builder --win"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "development": [
            "chrome 108"
        ],
        "production": [
            "chrome 108"
        ]
    },
    "pnpm": {
        "packageExtensions": {
            "@zardoy/electron-esbuild": {
                "dependencies": {
                    "esbuild": "^0.16.9"
                }
            }
        },
        "ignoreMissing": [
            "*"
        ],
        "allowAny": [
            "*"
        ]
    }
}
